// Test file for TASK-014 attribute parsing

#[no_learn]
fn safety_check(x: i64) -> bool {
    x > 0
}

#[no_gradient]
fn clamp(value: i64, min: i64, max: i64) -> i64 {
    if value < min {
        return min;
    }
    if value > max {
        return max;
    }
    value
}

#[invariant]
fn bounds_ok(x: i64) -> bool {
    x >= 0
}

#[no_learn]
#[no_gradient]
fn critical_op(a: i64, b: i64) -> i64 {
    a + b
}

fn main() -> i64 {
    let safe: bool = safety_check(10);
    let clamped: i64 = clamp(50, 0, 100);
    let ok: bool = bounds_ok(5);
    let result: i64 = critical_op(1, 2);

    if safe {
        println("Safety check passed");
    }

    println(f"Clamped value: {clamped}");
    println(f"Result: {result}");

    0
}
