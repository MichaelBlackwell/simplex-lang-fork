// Phase 36 Test: P1.5 - impl Trait parsing (simplified)

struct Number {
    value: i64
}

impl Number {
    fn get_value(&self) -> i64 {
        self.value
    }
}

// Return impl Trait - currently just returns concrete type
fn make_number(x: i64) -> Number {
    Number { value: x }
}

fn main() -> i64 {
    print("Testing P1.5: impl Trait return types");

    let n: Number = make_number(42);
    if n.get_value() != 42 {
        print("FAIL: struct method call");
        return 1;
    }
    print("  PASS: struct creation and method");

    print("All P1.5 tests passed!");
    0
}
