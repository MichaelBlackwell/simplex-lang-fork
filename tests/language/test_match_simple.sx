// Simple match test without guards
fn test_match_literal() -> i64 {
    let x: i64 = 5;
    match x {
        1 => 10,
        5 => 50,
        _ => 0
    }
}

fn test_match_wildcard() -> i64 {
    let x: i64 = 5;
    match x {
        _ => 42
    }
}

fn main() -> i64 {
    let r1: i64 = test_match_literal();
    let r2: i64 = test_match_wildcard();
    r1 + r2  // Should be 50 + 42 = 92
}
