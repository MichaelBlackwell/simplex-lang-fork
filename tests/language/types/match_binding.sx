// Test: Match pattern binding extraction

enum Option<T> {
    Some(T),
    None
}

fn extract_value(opt: Option<i64>) -> i64 {
    match opt {
        Option::Some(x) => x,
        Option::None => 0,
    }
}

fn main() -> i64 {
    let some_val: Option<i64> = Option::Some(42);
    let none_val: Option<i64> = Option::None;
    
    let r1 = extract_value(some_val);
    if r1 == 42 {
        println("PASS: extracted Some(42)");
    }
    
    let r2 = extract_value(none_val);
    if r2 == 0 {
        println("PASS: None returned default");
    }
    
    0
}
