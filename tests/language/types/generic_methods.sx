// Test: Generic method resolution

enum Option<T> {
    None,
    Some(T),
}

fn main() -> i64 {
    let opt: Option<i64> = Option::Some(42);
    
    // Test Option methods
    if opt.is_some() {
        println("PASS: Option::is_some()");
    }
    
    let val = opt.unwrap();
    if val == 42 {
        println("PASS: Option::unwrap()");
    }
    
    0
}
