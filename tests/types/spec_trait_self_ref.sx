// Test: &self in trait methods

trait Describable {
 fn describe(&self) -> i64;
 fn get_value(&self) -> i64;
}

struct Item {
 value: i64,
}

impl Describable for Item {
 fn describe(&self) -> i64 {
 self.value
 }
 
 fn get_value(&self) -> i64 {
 self.value * 2
 }
}

fn main() -> i64 {
 let item = Item { value: 21 };
 
 let d = item.describe();
 if d == 21 {
 println("PASS: &self method call");
 }
 
 let v = item.get_value();
 if v == 42 {
 println("PASS: &self method with computation");
 }
 
 0
}
