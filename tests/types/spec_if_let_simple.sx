// Test: Simple if-let pattern matching

enum Option<T> {
 None,
 Some(T),
}

fn test_if_let() -> i64 {
 let opt: Option<i64> = Option::Some(100);
 
 if let Option::Some(val) = opt {
 val
 } else {
 0
 }
}

fn main() -> i64 {
 let result = test_if_let();
 if result == 100 {
 println("PASS: if-let returns correct value");
 }
 0
}
