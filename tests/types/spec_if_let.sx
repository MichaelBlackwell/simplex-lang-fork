// Test: if-let pattern matching

enum Option<T> {
 None,
 Some(T),
}

fn main() -> i64 {
 let opt: Option<i64> = Option::Some(42);
 
 if let Option::Some(x) = opt {
 if x == 42 {
 println("PASS: if-let extracted value");
 }
 }
 
 let none: Option<i64> = Option::None;
 if let Option::Some(y) = none {
 println("FAIL: should not match None");
 } else {
 println("PASS: if-let else branch for None");
 }
 
 0
}
