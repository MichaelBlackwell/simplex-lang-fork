// Test: Option and Result construction and matching

enum Option<T> {
 None,
 Some(T),
}

enum Result<T, E> {
 Ok(T),
 Err(E),
}

fn main() -> i64 {
 // Test Option
 let some_val: Option<i64> = Some(42);
 let none_val: Option<i64> = None;
 
 match some_val {
 Some(x) => {
 if x == 42 {
 println("PASS: Some(42) = 42");
 }
 },
 None => println("FAIL"),
 }
 
 match none_val {
 Some(_) => println("FAIL"),
 None => println("PASS: None match = 99"),
 }
 
 // Test Result
 let ok_val: Result<i64, i64> = Ok(100);
 let err_val: Result<i64, i64> = Err(200);
 
 match ok_val {
 Ok(x) => {
 if x == 100 {
 println("PASS: Ok(100) = 100");
 }
 },
 Err(_) => println("FAIL"),
 }
 
 match err_val {
 Ok(_) => println("FAIL"),
 Err(e) => {
 if e == 200 {
 println("PASS: Err(200) = 200");
 }
 },
 }
 
 0
}
