// Simplified if-let test

enum Option<T> {
    None,
    Some(T),
}

fn get_some() -> Option<i64> {
    Some(42)
}

fn test_if_let() -> i64 {
    if let Some(x) = get_some() {
        x
    } else {
        0
    }
}

fn main() -> i64 {
    if test_if_let() == 42 {
        print("PASS: if-let extracts value");
        0
    } else {
        print("FAIL: if-let should return 42");
        1
    }
}
