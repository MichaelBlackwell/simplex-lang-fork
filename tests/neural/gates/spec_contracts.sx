// Neural gate test - contracts

neural_gate gateA(x: f64) -> bool {
 x > 0.5
}

neural_gate gateB(x: f64) -> bool
 requires x > 0.5
{
 x > 0.95
}

fn main() -> i64 {
 println("Test: Neural gates with contracts");

 neural_set_training_mode(0);

 println("Testing gateA...");
 let r1: f64 = gateA(0.8);
 println("Called gateA");

 println("Testing gateB...");
 let r2: f64 = gateB(0.9); // 0.9 meets requires (> 0.5), but < 0.95
 println("Called gateB");

 if r1 > 0.5 && r2 < 0.5 {
 println("ALL TESTS PASSED");
 return 0;
 }

 println("TEST FAILED");
 1
}
