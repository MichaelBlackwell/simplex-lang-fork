fn main() -> i64 {
 println("Testing negative float literals");

 let s: f64 = neural_sigmoid(-5.0);
 println("Called sigmoid(-5.0)");

 // sigmoid(-5) = 1 / (1 + e^5) = 0.0067...
 // Should be < 0.01
 if s < 0.01 {
 println("PASS: sigmoid(-5) < 0.01");
 return 0;
 }

 // Debug: print what we got
 if s > 0.99 {
 println("s > 0.99 - sign might be wrong?");
 } else if s > 0.5 {
 println("s > 0.5 - unexpected");
 } else {
 println("0.01 <= s <= 0.5");
 }

 println("FAIL");
 1
}
