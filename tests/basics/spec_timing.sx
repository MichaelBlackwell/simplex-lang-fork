// Test timing intrinsics 
// Expected: Prints timing info and returns 0

fn main() -> i64 {
 let start = get_time_ms();

 // Do some work
 var sum: i64 = 0;
 var i: i64 = 0;
 while i < 100000 {
 sum = sum + i;
 i = i + 1;
 }

 let end = get_time_ms();
 let elapsed = end - start;

 println(string_concat("Loop time (ms): ", int_to_string(elapsed)));
 println(string_concat("Sum: ", int_to_string(sum)));

 // Test microsecond timing
 let start_us = get_time_us();
 let end_us = get_time_us();
 println(string_concat("Timer overhead (us): ", int_to_string(end_us - start_us)));

 0
}
