// Test simple enum without data

enum Color {
    Red,
    Green,
    Blue,
}

fn main() -> i64 {
    print("Creating Color::Green...\n");
    let c = Color::Green;
    print("Created, now matching...\n");

    let result = match c {
        Color::Red => {
            print("Matched Red\n");
            1
        },
        Color::Green => {
            print("Matched Green\n");
            2
        },
        Color::Blue => {
            print("Matched Blue\n");
            3
        },
    };

    print("Result: ");
    print_i64(result);
    print("\n");
    result
}
