// Test loop with break value

fn test_loop_break() -> i64 {
    var i = 0;
    let result = loop {
        i = i + 1;
        if i == 5 {
            break i * 10;
        }
    };
    result  // Should be 50
}

fn main() -> i64 {
    let result = test_loop_break();
    print("Loop result: ");
    print_i64(result);
    print("\n");

    if result == 50 {
        print("loop_break: PASS\n");
        1
    } else {
        print("loop_break: FAIL\n");
        0
    }
}
