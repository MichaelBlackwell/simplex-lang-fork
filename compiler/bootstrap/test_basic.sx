// Test 1: Basic functions and structs

fn add(x: i64, y: i64) -> i64 {
    x + y
}

struct Point {
    x: i64,
    y: i64,
}

fn test_struct() -> i64 {
    let p = Point { x: 10, y: 20 };
    p.x + p.y
}

fn main() -> i64 {
    let result: i64 = add(10, 20);
    let struct_result: i64 = test_struct();
    print("Basic test passed!\n");
    result + struct_result
}
