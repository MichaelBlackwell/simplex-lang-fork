// Test enum variant constructors

enum Status {
    Ok(i64),
    Error,
}

fn test_enum_creation() -> i64 {
    let s = Status::Ok(42);
    match s {
        Status::Ok(v) => v,
        Status::Error => 0,
    }
}

fn main() -> i64 {
    if test_enum_creation() == 42 {
        print("enum: PASS\n");
        1
    } else {
        print("enum: FAIL\n");
        0
    }
}
