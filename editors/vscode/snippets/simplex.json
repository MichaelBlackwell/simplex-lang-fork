{
  "Function": {
    "prefix": "fn",
    "body": [
      "fn ${1:name}(${2:params}) -> ${3:i64} {",
      "    ${0}",
      "}"
    ],
    "description": "Define a function"
  },
  "Main Function": {
    "prefix": "main",
    "body": [
      "fn main() -> i64 {",
      "    ${0}",
      "    0",
      "}"
    ],
    "description": "Define main entry point"
  },
  "Async Function": {
    "prefix": "async",
    "body": [
      "async fn ${1:name}(${2:params}) -> ${3:i64} {",
      "    ${0}",
      "}"
    ],
    "description": "Define an async function"
  },
  "Struct": {
    "prefix": "struct",
    "body": [
      "struct ${1:Name} {",
      "    ${2:field}: ${3:i64},",
      "}"
    ],
    "description": "Define a struct"
  },
  "Enum": {
    "prefix": "enum",
    "body": [
      "enum ${1:Name} {",
      "    ${2:Variant1},",
      "    ${3:Variant2},",
      "}"
    ],
    "description": "Define an enum"
  },
  "Enum with Generics": {
    "prefix": "enumg",
    "body": [
      "enum ${1:Name}<${2:T}> {",
      "    ${3:None},",
      "    ${4:Some}($2),",
      "}"
    ],
    "description": "Define a generic enum"
  },
  "Trait": {
    "prefix": "trait",
    "body": [
      "trait ${1:Name} {",
      "    fn ${2:method}(&self) -> ${3:i64};",
      "}"
    ],
    "description": "Define a trait"
  },
  "Impl Block": {
    "prefix": "impl",
    "body": [
      "impl ${1:Type} {",
      "    fn ${2:new}(${3:params}) -> Self {",
      "        ${0}",
      "    }",
      "}"
    ],
    "description": "Implement methods for a type"
  },
  "Impl Trait": {
    "prefix": "implt",
    "body": [
      "impl ${1:Trait} for ${2:Type} {",
      "    fn ${3:method}(&self) -> ${4:i64} {",
      "        ${0}",
      "    }",
      "}"
    ],
    "description": "Implement a trait for a type"
  },
  "Actor": {
    "prefix": "actor",
    "body": [
      "actor ${1:Name} {",
      "    ${2:state}: ${3:i64} = ${4:0};",
      "",
      "    receive {",
      "        ${5:Message} => {",
      "            ${0}",
      "        }",
      "    }",
      "}"
    ],
    "description": "Define an actor"
  },
  "Actor with Init": {
    "prefix": "actori",
    "body": [
      "actor ${1:Name} {",
      "    ${2:state}: ${3:i64} = ${4:0};",
      "",
      "    init {",
      "        ${5}",
      "    }",
      "",
      "    receive {",
      "        ${6:Message} => {",
      "            ${0}",
      "        }",
      "    }",
      "}"
    ],
    "description": "Define an actor with init block"
  },
  "Specialist": {
    "prefix": "specialist",
    "body": [
      "specialist ${1:Name} {",
      "    model: \"${2:gpt-4}\";",
      "    temperature: ${3:0.7};",
      "",
      "    fn ${4:process}(${5:input}: String) -> String {",
      "        infer(${0})",
      "    }",
      "}"
    ],
    "description": "Define an AI specialist"
  },
  "Hive": {
    "prefix": "hive",
    "body": [
      "hive ${1:Name} {",
      "    specialists: [${2:Spec1}, ${3:Spec2}];",
      "    coordinator: ${4:Coordinator};",
      "",
      "    fn ${5:process}(${6:input}: String) -> String {",
      "        ${0}",
      "    }",
      "}"
    ],
    "description": "Define a cognitive hive"
  },
  "Match": {
    "prefix": "match",
    "body": [
      "match ${1:value} {",
      "    ${2:Pattern} => ${3:expr},",
      "    _ => ${0},",
      "}"
    ],
    "description": "Pattern matching expression"
  },
  "Match Option": {
    "prefix": "matcho",
    "body": [
      "match ${1:option} {",
      "    Some(${2:x}) => ${3:expr},",
      "    None => ${0},",
      "}"
    ],
    "description": "Match on Option type"
  },
  "Match Result": {
    "prefix": "matchr",
    "body": [
      "match ${1:result} {",
      "    Ok(${2:val}) => ${3:expr},",
      "    Err(${4:e}) => ${0},",
      "}"
    ],
    "description": "Match on Result type"
  },
  "If Let": {
    "prefix": "iflet",
    "body": [
      "if let ${1:Some(x)} = ${2:expr} {",
      "    ${0}",
      "}"
    ],
    "description": "If let pattern matching"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for ${1:item} in ${2:iter} {",
      "    ${0}",
      "}"
    ],
    "description": "For loop"
  },
  "For Range": {
    "prefix": "forr",
    "body": [
      "for ${1:i} in ${2:0}..${3:10} {",
      "    ${0}",
      "}"
    ],
    "description": "For loop with range"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "while ${1:condition} {",
      "    ${0}",
      "}"
    ],
    "description": "While loop"
  },
  "Let Binding": {
    "prefix": "let",
    "body": "let ${1:name}: ${2:i64} = ${0};",
    "description": "Variable binding"
  },
  "Let Mutable": {
    "prefix": "letm",
    "body": "let mut ${1:name}: ${2:i64} = ${0};",
    "description": "Mutable variable binding"
  },
  "Closure": {
    "prefix": "closure",
    "body": "|${1:params}| ${0}",
    "description": "Closure expression"
  },
  "Closure with Block": {
    "prefix": "closureb",
    "body": [
      "|${1:params}| {",
      "    ${0}",
      "}"
    ],
    "description": "Closure with block body"
  },
  "Spawn Actor": {
    "prefix": "spawn",
    "body": "let ${1:actor} = spawn ${2:ActorName} {};",
    "description": "Spawn an actor"
  },
  "Send Message": {
    "prefix": "send",
    "body": "send ${1:actor} ${2:Message};",
    "description": "Send message to actor"
  },
  "Ask Actor": {
    "prefix": "ask",
    "body": "let ${1:result} = ask ${2:actor} ${3:Message};",
    "description": "Ask actor and wait for response"
  },
  "Println": {
    "prefix": "println",
    "body": "println(${0});",
    "description": "Print with newline"
  },
  "F-string": {
    "prefix": "fstring",
    "body": "f\"${1:text} {${2:var}}\"",
    "description": "Interpolated format string"
  },
  "Assert": {
    "prefix": "assert",
    "body": "assert(${0});",
    "description": "Assert condition"
  },
  "Assert Eq": {
    "prefix": "asserteq",
    "body": "assert_eq(${1:left}, ${2:right});",
    "description": "Assert equality"
  },
  "Test Function": {
    "prefix": "test",
    "body": [
      "#[test]",
      "fn test_${1:name}() {",
      "    ${0}",
      "}"
    ],
    "description": "Test function"
  },
  "Documentation Comment": {
    "prefix": "///",
    "body": [
      "/// ${1:Brief description}",
      "///",
      "/// # Arguments",
      "///",
      "/// * `${2:arg}` - ${3:Description}",
      "///",
      "/// # Returns",
      "///",
      "/// ${4:Return description}",
      "///",
      "/// # Example",
      "///",
      "/// ```simplex",
      "/// ${0}",
      "/// ```"
    ],
    "description": "Documentation comment block"
  }
}
